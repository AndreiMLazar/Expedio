{
  "name": "expedio",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndreiMLazar/Expedio.git"
  },
  "homepage": "https://github.com/AndreiMLazar/Expedio#readme",
  "keywords": [
    "Expedio"
  ],
  "author": "Andrei Lazar (https://www.linkedin.com/in/mihai-andrei-lazar-3b10227b/)",
  "scripts": {
    "ng": "ng",
    "start": "node ./backend/server.js --no-deprecation",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "backend": "nodemon ./backend/server.js --no-deprecation",
    "backend-debug": "nodemon ./backend/server.js --inspect --no-deprecation",
    "node-debug": "node --nolazy --inspect-brk=9229 ./backend/server.js --no-deprecation",
    "prod": "ng build --prod && npm run ngsw-config && npm run ngsw-copy",
    "ngsw-config": "node_modules/.bin/ngsw-config backend/expedioUI ngsw-config.json",
    "ngsw-copy": "cp node_modules/@angular/service-worker/ngsw-worker.js backend/expedioUI/"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^0.800.1",
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/cli": "^8.0.1",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/language-service": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/pwa": "^0.800.1",
    "@angular/router": "~8.0.0",
    "@angular/service-worker": "~8.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "hammerjs": "^2.0.8",
    "mongoose": "^5.5.13",
    "mongoose-unique-validator": "^2.0.3",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "typescript": "3.4.5",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nodemon": "^1.19.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0"
  }
}
